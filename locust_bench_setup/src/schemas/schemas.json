{
  "openai": {
    "endpoint": "/v1/completions",
    "body": {
      "model": "{{model}}",
      "prompt": "{{prompt}}",
      "max_tokens": "{{max_tokens}}",
      "temperature": 0,
      "frequency_penalty": 0.8,
      "ignore_eos": true,
      "stream": true,
      "stream_options": {
        "include_usage": true
      }
    },
    "response": {
      "text": "choices[0].text",
      "prompt_tokens": "usage.prompt_tokens",
      "completion_tokens": "usage.completion_tokens"
    }
  }
}